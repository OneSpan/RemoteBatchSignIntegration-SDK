<signed-approval-token>	<approval id="${approvalId}" type="fw/approval">		<signed-approval-data id="${approvalId}_SIGNED_DATA">			<approval-attributes>				<index>-1</index>				<version/>				<is-revalidator>false</is-revalidator>				<comment/>				<affirmation/>				<reason/>				<timezone>240</timezone>				<signingmethod>certificate</signingmethod>				<fips-compliance>None</fips-compliance>				<timestamp>${acceptTime}</timestamp>			</approval-attributes>			<user-info>				<email/>				<name>					<name></name>					<title/>				</name>				<company/>			</user-info>			<new-autograph type="fw/legacy-autograph">				<friendlyName/>				<description/>				<imageBytes></imageBytes>			</new-autograph>		</signed-approval-data>	</approval>	<protocol>   		<version>4.0</version>   		<max-supported-version>4.0</max-supported-version>   		<min-required-version>3.0</min-required-version>   		<signing-service>RemotePkiCms</signing-service>   		<data>${nonce}</data>   	</protocol>   	<extensions>   		<item>   			<id>1.1.1</id>   			<critical>false</critical>   			<data>${signaturePlaceholderSize}</data>   		</item>   	</extensions></signed-approval-token>